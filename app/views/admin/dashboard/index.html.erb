<div class="container">
    <h1 class="text-center">All My Recipes</h1>
    <div class="row">
        <div class="list-group">
            <% @recipes.each do | recipe | %> 
                <a href="/admin/editRecipe/<%= recipe.id %>" class="list-group-item list-group-item-action" aria-current="true">
                    <%= recipe.title %>
                </a>
            <% end %> 
        </div>          
    </div>
</div>